#!/bin/bash
#-a stands for "archive mode" which preserves permissions, symlinks, etc.
#-v means "verbose", which will display the syncing process details.
#-r indicates "recursive", necessary to copy directories and their contents.
cp /Users/marcio/DevLibs/node-firebird2/index.d.ts /Users/marcio/Dev/heitz_CS1.1.3/heitz-ws/node_modules/node-firebird2/index.d.ts
rsync -avr /Users/marcio/DevLibs/node-firebird2/lib/ /Users/marcio/Dev/heitz_CS1.1.3/heitz-ws/node_modules/node-firebird2/lib/
